{
    "name": "gingerframework/gingerframework",
    "description": "Ginger Workflow Framework",
    "type": "library",
    "keywords": [
        "framework",
        "GingerWF",
        "workflow engine"
    ],
    "homepage": "https://github.com/gingerframework",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.4.8",
        "beberlei/assert": "~2.0",
        "codeliner/php-equalsbuilder": "~1.1",
        "codeliner/array-reader": "~1.1",
        "prooph/service-bus" : "~1.0",
        "prooph/event-store" : "~1.0",
        "prooph/event-sourcing" : "~1.0",
        "zendframework/zend-stdlib" : "~2.3",
        "rhumsaa/uuid" : "~2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7"
    },
    "suggest": {
    },
    "autoload": {
        "psr-0": {
            "Ginger\\": "library/",
            "GingerTest\\": "tests/"
        }
    },
    "replace": {
        "gingerframework/ginger-type": "self.version",
        "gingerframework/ginger-message": "self.version"
    }
}