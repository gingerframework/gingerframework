{
    "name": "prooph/processing",
    "description": "Workflow Processing Framework",
    "type": "library",
    "keywords": [
        "prooph",
        "workflow",
        "processing"
    ],
    "homepage": "https://github.com/prooph/processing",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.5",
        "prooph/service-bus" : "~2.0",
        "prooph/event-store" : "~2.0",
        "prooph/event-sourcing" : "~1.0",
        "prooph/common" : "~1.0",
        "beberlei/assert": "~2.0",
        "codeliner/php-equalsbuilder": "~1.1",
        "codeliner/array-reader": "~1.1",
        "zendframework/zend-stdlib" : "~2.3",
        "zendframework/zend-servicemanager" : "~2.3",
        "zendframework/zend-eventmanager" : "~2.3",
        "zfcampus/zf-console" : "~1.0",
        "rhumsaa/uuid" : "~2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7",
        "prooph/event-store-zf2-adapter" : "~0.3"
    },
    "suggest": {
    },
    "autoload": {
        "psr-4": {
            "Prooph\\Processing\\": "library/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Prooph\\ProcessingTest\\": "tests/ProcessingTest",
            "Prooph\\ProcessingExample\\": "examples/ProcessingExample"
        }
    },
    "replace": {
        "prooph/processing-type": "self.version",
        "prooph/processing-message": "self.version"
    },
    "bin": ["./library/Console/bin/processing"]
}
