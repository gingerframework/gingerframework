{
    "name": "gingerframework/gingerframework",
    "description": "Ginger Workflow Framework",
    "type": "library",
    "keywords": [
        "framework",
        "gingerframework",
        "workflow engine"
    ],
    "homepage": "https://github.com/gingerframework",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.4.8",
        "beberlei/assert": "~2.0",
        "codeliner/php-equalsbuilder": "~1.1",
        "codeliner/array-reader": "~1.1",
        "prooph/service-bus" : "~2.0",
        "prooph/event-store" : "~2.0",
        "prooph/event-sourcing" : "~1.0",
        "zendframework/zend-stdlib" : "~2.3",
        "zendframework/zend-servicemanager" : "~2.3",
        "zendframework/zend-eventmanager" : "~2.3",
        "zfcampus/zf-console" : "~1.0",
        "rhumsaa/uuid" : "~2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7",
        "prooph/event-store-zf2-adapter" : "~0.3"
    },
    "suggest": {
    },
    "autoload": {
        "psr-0": {
            "Ginger\\": "library/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "GingerTest\\": "tests/",
            "GingerExample\\": "examples/"
        }
    },
    "replace": {
        "gingerframework/ginger-type": "self.version",
        "gingerframework/ginger-message": "self.version"
    },
    "bin": ["./library/Ginger/Console/bin/ginger"]
}
